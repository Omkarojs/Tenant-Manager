        android:label="@string/app_name"
        android:roundIcon="@drawable/rent"
        android:supportsRtl="true"
        android:theme="@style/Theme.TenantManager">

        <activity android:name="com.omkar.tenantmanager.activity.AddNewTenantActivity"
            android:exported="true">

        </activity>

        <activity android:name="com.omkar.tenantmanager.activity.FirstScreenActivity"
            android:exported="true">

        </activity>

        <activity android:name="com.omkar.tenantmanager.activity.PaymentListActivity"
            android:exported="true">

        </activity>

        <activity android:name="com.omkar.tenantmanager.activity.RentListActivity"
            android:exported="true">

