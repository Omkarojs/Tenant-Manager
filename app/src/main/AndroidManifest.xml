        <activity android:name="com.omkar.tenantmanager.activity.TenantListActivity"
            android:exported="true">

        </activity>

        <activity android:name="com.omkar.tenantmanager.activity.PaymentsActivity"
            android:exported="true">

        </activity>


        <activity android:name="com.omkar.tenantmanager.activity.AddNewRentActivity"
            android:exported="true">

        </activity>

        <activity android:name="com.omkar.tenantmanager.activity.UpdateActivity"
            android:exported="true">

        </activity>

        <activity android:name="com.omkar.tenantmanager.activity.UpdateReceiptActivity"
            android:exported="true">

        </activity>
        <activity android:name="com.omkar.tenantmanager.activity.UpdateRentActivity"
            android:exported="true">

        </activity>

        <activity android:name="com.omkar.tenantmanager.activity.TenantReceiptActivity"
            android:exported="true">

        </activity>

        <activity android:name="com.omkar.tenantmanager.activity.SplashScreenActivity"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

    </application>

</manifest>